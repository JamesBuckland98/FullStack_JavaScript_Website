{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "max-len": [1,1200,2,{"ignoreComments": true}],
        "no-console":[1],
        "no-unused-vars":[1],
        "no-loop-func":[1],
        "indent": [
            "error",
            "tab"
        ],
        "quotes": [
            "error",
            "single"
        ]
    }
}